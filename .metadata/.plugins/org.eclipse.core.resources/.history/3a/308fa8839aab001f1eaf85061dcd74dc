package com.ar.stack;

public class Stack<T> {

	private Node top = null;
	
	/*Internal Class*/
	class Node{
		T item;
		Node next;
	}
	
	
	
	/*Insert a new element to the stack*/
	public void push(T data) {
		/*Change references*/
		Node previous = top;
		top = new Node();
		top.item = data;
		top.next = previous;
	}
	
	
}
